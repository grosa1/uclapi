{
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 4,
  "js": {
    "groupName": "all minor dependencies",
    "separateMajorMinor": false,
    "groupSlug": "all",
    "packageRules": [
      {
    "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all javascript non-major dependencies",
      "groupSlug": "all-js-minor-patch"
      }
    ],
    "lockFileMaintenance": {
      "enabled": false
    },
  },
  "python": {
   "groupName": "all minor dependencies",
    "separateMajorMinor": false,
    "groupSlug": "all",
    "packageRules": [
      {
    "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all python non-major dependencies",
      "groupSlug": "all-py-minor-patch"
      }
    ],
    "lockFileMaintenance": {
      "enabled": false
    },
  }
}
