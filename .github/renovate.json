{
  "extends": [
    "config:base"
  ],
  "js": {
    "managerBranchPrefix": "js-"
  },
  "python": {
    "managerBranchPrefix": "python-"
  },
  "packageRules": [
    {
      "matchPackagePatterns": [
        "*"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "groupName": "all non-major dependencies",
      "groupSlug": "all-minor-patch"
    }
  ]
}
